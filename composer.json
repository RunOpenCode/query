{
    "name": "runopencode/query",
    "description": "Write your dynamic queries in separate files, execute them in your repository.",
    "license": "MIT",
    "authors": [
        {
            "name": "Nikola Svitlica a.k.a TheCelavi",
            "email": "thecelavi@runopencode.com"
        }
    ],
    "require": {
        "php": ">=8.4",
        "doctrine/dbal": "^4.0",
        "psr/cache": "^3.0",
        "psr/log": "^2.0|^3.0",
        "symfony/cache": "^7.0",
        "symfony/finder": "^7.0",
        "thecodingmachine/safe": "^3.3",
        "twig/twig": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "RunOpenCode\\Component\\Query\\": "src"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "repositories": [
        {
            "type": "path",
            "url": "../../Component/Dataset",
            "options": {
                "symlink": false
            }
        },
        {
            "type": "path",
            "url": "../../Component/Bitmask",
            "options": {
                "symlink": false
            }
        },
        {
            "type": "path",
            "url": "../../Component/Metadata",
            "options": {
                "symlink": false
            }
        },
        {
            "type": "path",
            "url": "../../Bundle/MetadataBundle",
            "options": {
                "symlink": false
            }
        },
        {
            "type": "path",
            "url": "../../Bundle/QueryBundle",
            "options": {
                "symlink": false
            }
        }
    ],
    "config": {
        "sort-packages": true
    },
    "version": "0.2",
    "minimum-stability": "dev",
    "prefer-stable": true
}
